{
  "name": "neo4j-cypher-runner",
  "displayName": "neo4j-cypher-runner",
  "description": "Run cypher queries directly from VSCode",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:neo4j-cypher-runner.run-query",
    "onCommand:neo4j-cypher-runner.select-database"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "neo4j-cypher-runner.run-query",
        "title": "Neo4j Cypher Runner: Run Query"
      },
      {
        "command": "neo4j-cypher-runner.select-database",
        "title": "Neo4j Cypher Runner: Select Database"
      }
    ],
    "configuration": [
      {
        "title": "Neo4j Cypher Runner",
        "properties": {
          "neo4j-cypher-runner.databases": {
            "type": "list",
            "title": "Databases",
            "default": [
              {
                "name": "My DB",
                "url": "neo4j://localhost",
                "authToken": {
                  "principal": "neo4j",
                  "credentials": "pass",
                  "scheme": "basic"
                }
              }

            ],
            "description": "Complete functions with their parameter signature."
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "neo4j-driver": "^4.3.0"
  }
}